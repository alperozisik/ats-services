
{
    "apiName":"ats",
    "apiVersion":"1.0",
    "apiId":"0ccc99a8-bb4d-4dbc-9746-6d089230bcea",
    "baseUrl":"http://<MCS_SERVER>:<MCS_PORT>/",
    "authorization":{
        "mobileBackendID":"<YOUR_MOBILE_BACKEND_ID>",
        "anonymousKey":"<MBE_ANONYMOUS_KEY>",
        "oauth":{
            "tokenEndpoint":"<MBE_OAUTH_TOKEN_ENDPOINT>",
            "clientId":"<MBE_OAUTH_CLIENT_ID>",
            "clientSecret":"<MBE_OAUTH_CLIENT_ID>"
        }
    },
    "container":{
        "oracleMobileApiName":"OracleMobileAPI",
        "port":4000
    },
    "deploy":{
        "tempDirectory":"/tmp/mcs-deploy"
    },
    "tests":{
        "getAppointmentDoctorIdPeriodDate":{
            "method":"GET",
            "resource":"/appointment/:doctorId/:period/:date",
            "uriParameters":{
                "doctorId":"<PARAMETER_VALUE>",
                "period":"<PARAMETER_VALUE>",
                "date":"<PARAMETER_VALUE>"
            }
        },
        "getLabResultsOrderNo":{
            "method":"GET",
            "resource":"/lab/results/:orderNo",
            "uriParameters":{
                "orderNo":"<PARAMETER_VALUE>"
            }
        },
        "getClinic":{
            "method":"GET",
            "resource":"/clinic"
        },
        "getPatientPatientIdTimeline":{
            "method":"GET",
            "resource":"/patient/:patientId/timeline",
            "uriParameters":{
                "patientId":"<PARAMETER_VALUE>"
            }
        },
        "getPatientPatientId":{
            "method":"GET",
            "resource":"/patient/:patientId",
            "uriParameters":{
                "patientId":"<PARAMETER_VALUE>"
            }
        },
        "getAppointmentCategory":{
            "method":"GET",
            "resource":"/appointment/category"
        },
        "getAppointmentPeriod":{
            "method":"GET",
            "resource":"/appointment/period"
        },
        "getClinicClinicNoDoctors":{
            "method":"GET",
            "resource":"/clinic/:clinicNo/doctors",
            "uriParameters":{
                "clinicNo":"<PARAMETER_VALUE>"
            }
        },
        "postAppointment":{
            "method":"POST",
            "resource":"/appointment",
            "payload":{
                "doctorId":124,
                "slotSerial":"446411",
                "categorycode":368,
                "patientId":21212,
                "notificationToken":"xxyyzz"
            }
        },
        "postNotification":{
            "method":"POST",
            "resource":"/notification",
            "payload":{
                "message":"text"
            }
        },
        "postPatientLogin":{
            "method":"POST",
            "resource":"/patient/login",
            "payload":{
                "username":"smartface",
                "password":"123456",
                "deviceType":"iOS",
                "notificationToken":"1"
            }
        },
        "getAppointmentAvailableClinicNoDoctorIdYearMonth":{
            "method":"GET",
            "resource":"/appointment/available/:clinicNo/:doctorId/:yearMonth",
            "uriParameters":{
                "clinicNo":"<PARAMETER_VALUE>",
                "doctorId":"<PARAMETER_VALUE>",
                "yearMonth":"<PARAMETER_VALUE>"
            }
        }
    }
}